{
  "name": "rescript-express-recipes",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rescript clean",
    "build": "rescript",
    "watch": "rescript build -with-deps -w",
    "start": "node src/Index.bs.js",
    "test": "onchange --initial '{tests,src}/*.js' -- pta 'tests/*.test.bs.js'",
    "test:coverage": "c8 --all pta 'tests/*.test.bs.js'",
    "test:htmlcoverage": "c8 ---reporter html --all pta 'tests/*.test.bs.js'"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bs-express": "github:reasonml-community/bs-express",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@dusty-phillips/rescript-zora": "^2.0.0",
    "c8": "^7.7.2",
    "onchange": "^7.1.0",
    "pta": "^1.0.0",
    "rescript": "^9.1.4",
    "zora": "^5.0.0"
  }
}
